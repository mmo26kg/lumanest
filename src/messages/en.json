            "items": {
                "shipping": {
                    "question": "What are your shipping options?",
                    "answer": "We offer free standard shipping on orders over $500. Express shipping is available for $50. White glove delivery service is also available for large furniture pieces."
                },
                "warranty": {
                    "question": "What warranty do you offer?",
                    "answer": "All our furniture comes with a 5-year warranty covering manufacturing defects. Fabric and leather have a 2-year warranty for normal wear and tear."
                },
                "returns": {
                    "question": "What is your return policy?",
                    "answer": "You can return items within 30 days of delivery in original condition. Custom orders and personalized items are not eligible for return."
                },
                "customization": {
                    "question": "Do you offer customization options?",
                    "answer": "Yes, we offer various customization options including fabric choices, wood finishes, and size modifications. Contact us to discuss your specific needs."
                }
            }
