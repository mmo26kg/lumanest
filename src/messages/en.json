    "about": {
        "hero": {
            "title": "Lumanest Furniture",
            "subtitle": "Crafting Homes, Building Futures."
        },
        "story": {
            "title": "Our Story",
            "content": "Founded on the principles of timeless design and impeccable craftsmanship, Lumanest Furniture began with a simple idea: to create beautiful, lasting pieces that turn a house into a home. Our journey started in a small workshop, driven by a passion for woodworking and a commitment to sustainable practices. Today, we continue to honor that legacy, blending traditional techniques with modern aesthetics to bring you furniture that is not only stylish but also built to be part of your family's story for generations."
        },
        "mission": {
            "title": "Our Mission",
            "content": "To design and craft high-quality, sustainable furniture that enhances the beauty and comfort of everyday living."
        },
        "vision": {
            "title": "Our Vision",
            "content": "To be a global leader in innovative and eco-conscious home furnishings, inspiring a more thoughtful way of life."
        },
        "coreValues": {
            "title": "Our Core Values",
            "subtitle": "The principles that guide every cut, stitch, and decision we make.",
            "items": {
                "quality": {
                    "title": "Uncompromising Quality",
                    "description": "We use only the finest materials and time-honored techniques to ensure every piece is a masterpiece of durability and design."
                },
                "sustainable": {
                    "title": "Sustainable by Design",
                    "description": "Our commitment to the planet is reflected in our responsibly sourced materials and eco-friendly production processes."
                },
                "innovation": {
                    "title": "Timeless Innovation",
                    "description": "We blend classic aesthetics with modern functionality, creating furniture that is both beautiful and perfectly suited for contemporary life."
                }
            }
        },
        "craftsmanship": {
            "title": "Our Craftsmanship",
            "subtitle": "From raw material to the heart of your home, every piece tells a story of dedication.",
            "items": {
                "materials": {
                    "alt": "Close-up of solid oak wood planks",
                    "title": "Sourcing the Finest Materials",
                    "description": "Our journey begins with responsibly harvested solid wood. We hand-select each plank for its unique grain, character, and structural integrity, ensuring a foundation of unparalleled quality."
                },
                "finishing": {
                    "alt": "Artisan sanding a wooden chair",
                    "title": "Meticulous Hand-Finishing",
                    "description": "Skilled artisans bring the design to life. Each joint is perfected, each surface is sanded to a flawless finish, and every detail is scrutinized to meet our exacting standards of excellence."
                }
            }
        },
